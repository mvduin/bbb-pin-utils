vin 0.0a d0		-			-			vout 2 d7		vout 2 d23		uart 7 rxd		-			-			-			-			pwm 0 A			-			-			-			gpio 2.04		-×
vin 0.0a d1		-			-			vout 2 d6		vout 2 d22		uart 7 txd		-			-			-			-			pwm 0 B			-			-			-			gpio 2.05		-×
