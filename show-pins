vin 0.0a de		vin 0.0b hsync		-			vout 2 d17		vout 2 de		uart 6 rxd		-			timer 15		spi 2 sclk		kbd row 0		qep 0 A			-			-			-			gpio 2.00		-×
vin 0.0a fld		vin 0.0b vsync		-			-			vout 2 clk		uart 6 txd		-			timer 14		spi 2 d1		kbd row 1		qep 0 B			-			-			-			gpio 2.01		-×
